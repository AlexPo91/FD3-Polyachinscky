
{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true,
    "jest": true,
    "commonjs": true
  },
  "extends": [
    "airbnb","eslint:recommended", "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "parser": "babel-eslint"
}
